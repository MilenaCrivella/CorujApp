<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">
    <!--tools:showIn="@layout/app_bar_menu"
    tools:context="cf.milenaverissimo.corujapp.DevelopmentActivity"-->

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Ao nascer"
        android:id="@+id/title1"
        android:layout_marginTop="@dimen/text_margin"
        android:textColor="@color/colorPrimaryDark"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BCG"
        android:id="@+id/vaccine1"
        android:layout_below="@+id/title1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:buttonTint="@color/colorPrimaryDark"
        android:textColor="@color/colorText"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hepatite B - 1°dose "
        android:textColor="@color/colorText"
        android:id="@+id/vaccine2"
        android:layout_below="@+id/vaccine1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:buttonTint="@color/colorPrimaryDark"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="1° Mês"
        android:textColor="@color/colorPrimaryDark"
        android:id="@+id/title2"
        android:layout_below="@+id/vaccine2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hepatite B - 2°dose "
        android:id="@+id/vaccine3"
        android:layout_below="@+id/title2"
        android:textColor="@color/colorText"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:buttonTint="@color/colorPrimaryDark"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="2° Mês"
        android:id="@+id/title3"
        android:layout_below="@+id/vaccine3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@color/colorPrimaryDark"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pentavalente 1ªdose"
        android:id="@+id/vaccine4"
        android:layout_below="@+id/title3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@color/colorText"
        android:buttonTint="@color/colorPrimaryDark"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/colorText"
        android:text="Poliomielite 1ª dose"
        android:id="@+id/vaccine5"
        android:layout_below="@+id/vaccine4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:buttonTint="@color/colorPrimaryDark"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/colorText"
        android:text="Pneumocócica conjugada 1ª dose"
        android:id="@+id/vaccine6"
        android:layout_below="@+id/vaccine5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:buttonTint="@color/colorPrimaryDark"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/colorText"
        android:text="Rotavírus 1ª dose"
        android:id="@+id/vaccine7"
        android:layout_below="@+id/vaccine6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:buttonTint="@color/colorPrimaryDark"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/colorText"
        android:text="Hepatite B- 3° dose"
        android:id="@+id/vaccine8"
        android:layout_below="@+id/vaccine7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:buttonTint="@color/colorPrimaryDark"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="3° Mês"
        android:id="@+id/title4"
        android:layout_below="@+id/vaccine8"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@color/colorPrimaryDark"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Meningocócica C conjugada - 1ª dose"
        android:id="@+id/vaccine9"
        android:textColor="@color/colorText"
        android:layout_below="@+id/title4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:buttonTint="@color/colorPrimaryDark"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pentavalente 2ª dose"
        android:textColor="@color/colorText"
        android:id="@+id/vaccine10"
        android:layout_below="@+id/vaccine9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:buttonTint="@color/colorPrimaryDark"/>
</RelativeLayout>
